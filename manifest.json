{
  "manifest_version": 3,
  "name": "Goodreads → Obsidian",
  "version": "v.0.1.0",
  "description": "Save book info from Goodreads and import it to Obsidian.",
  "permissions": ["activeTab", "scripting", "tabs"],
  "host_permissions": ["*://*.goodreads.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Goodreads → Obsidian"
  }
}


